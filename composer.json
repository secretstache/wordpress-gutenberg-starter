{
  "version": "2.0.2",
  "name": "secretstache/wordpress-gutenberg-starter",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme based on the Sage framework",
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "roots/acorn": "^5.0",
    "roots/acorn-prettify": "^1.0.0",
    "secretstache/wordpress-core": "^1.0.0",
    "johnbillion/extended-cpts": "^4.3",
    "stoutlogic/acf-builder": "^1.9",
    "log1x/navi": "*"
  },
  "require-dev": {
    "laravel/pint": "^1.20",
    "symfony/process": "^v7.2.0"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-create-project-cmd": [
      "Composer\\Config::disableProcessTimeout",
      "App\\ComposerScripts::postCreateProject"
    ],
    "setup-static-boilerplate": [
      "App\\ComposerScripts::setupStaticBoilerplate"
    ],
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
