{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Accordion",
  "description": "",
  "name": "ssm/accordion",
  "keywords": [
    "accordion",
    "faq"
  ],
  "category": "ssm-templates",
  "example": {},
  "icon": "columns",
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "isOpenedByDefault": {
      "type": "boolean",
      "default": false
    },
    "dataSource": {
      "type": "string",
      "enum": [
        "none",
        "faq"
      ],
      "default": "none"
    },
    "queryType": {
      "type": "string",
      "enum": [
        "latest",
        "by_faq_category",
        "curated"
      ],
      "default": "latest"
    },
    "numberOfPosts": {
      "type": "number",
      "default": 6
    },
    "selectedCategories": {
      "type": "array",
      "default": []
    },
    "curatedPosts": {
      "type": "array",
      "default": []
    }
  },
  "render": "index"
}
