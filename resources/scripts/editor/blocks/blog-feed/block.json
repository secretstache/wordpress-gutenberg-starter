{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Blog Feed",
  "description": "",
  "name": "ssm/blog-feed",
  "keywords": [
    "blog",
    "feed",
    "bf"
  ],
  "category": "ssm-templates",
  "example": {},
  "icon": "columns",
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "queryType": {
      "type": "string",
      "enum": [
        "latest",
        "by_category",
        "curated"
      ],
      "default": "latest"
    },
    "curatedTerms": {
      "type": "array",
      "default": []
    },
    "curatedPosts": {
      "type": "array",
      "default": []
    },
    "numberOfPosts": {
      "type": "number",
      "default": -1
    },
    "isShowFilters": {
      "type": "boolean",
      "default": false
    }
  },
  "render": "index"
}
